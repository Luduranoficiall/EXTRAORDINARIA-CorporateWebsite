{
  "name": "corporate-website-development",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lucide-react": "^0.487.0"
  },
  "devDependencies": {
    "vite": "6.3.5",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "typescript": "^5.4.2",
  "@types/node": "^20.4.2",
    "eslint": "^8.50.0",
  "@types/sonner": "^1.0.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "prettier": "^3.0.0",
  "vitest": "^1.3.0",
  "@types/react": "^18.2.21",
  "@types/react-dom": "^18.2.7",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "chokidar-cli": "^3.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --host --port 8080 --strictPort",
    "build:all": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/build-all-languages.ps1",
    "py:run": "python ./languages/python/runner.py",
    "py:push": "python ./languages/python/gh_push_via_token.py",
  "node:crypto": "node ./languages/node/sample_crypto.js",
  "node:run-all": "node ./languages/node/run_all.js",
  "node:push": "node ./languages/node/gh_push_via_token.js",
  "node:runner": "node ./languages/node/runner.js",
    "preview:iframe": "vite --open public/iframe-preview.html",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "vitest",
    "auto-backup": "git checkout -B auto-backups && chokidar \"src/**/*\" -c \"git add -A && git commit -m 'auto: snapshot' || true\" --initial --debounce 10000",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}